{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "pytest: Calendar",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "console": "integratedTerminal"
    },
    {
      "name": "PROD: pytest: Calendar",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env.production"
    }
  ]
}
